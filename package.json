{
  "name": "replimap-mono",
  "version": "0.0.0",
  "private": true,
  "description": "RepliMap Monorepo - Cloud Infrastructure Visualization & Compliance",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "config:build": "pnpm --filter @replimap/config build",
    "config:check": "pnpm --filter @replimap/config check"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
